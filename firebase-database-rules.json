{
  "rules": {
    "active-sessions": {
      "$emailKey": {
        ".read": "auth != null",
        ".write": "auth != null",
        ".validate": "newData.hasChildren(['sessionId', 'loginTime', 'email'])"
      }
    },
    "authorized-emails": {
      "$emailKey": {
        ".read": "auth != null",
        ".write": "auth != null",
        ".validate": "newData.hasChildren(['email', 'addedAt', 'addedBy'])"
      }
    }
  }
}
